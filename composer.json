{
    "name": "ksfraser/ksf-rest-api",
    "description": "REST API Library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Fraser",
            "email": "kevin@ksfraser.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ksfraser/ksf-modules-dao": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.0"
    },
    "autoload": {
        "psr-4": {
            "Ksfraser\\Rest_API\\": "src/Ksfraser/Rest_API/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ksfraser\\Rest_API\\Test\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../ksf_ModulesDAO"
        }
    ],
    "scripts": {
        "test": "@php vendor/bin/phpunit"
    }
}